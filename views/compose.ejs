<%- include('partials/header.ejs') %>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="mb-3">
      <label>Title</label>
      <input type="text" class="form-control" name="postTitle" autocomplete="off">
    </div>

    <div class="mb-3">
        <label >Post</label>
        <textarea name="postBody" rows="10" cols="30" class="form-control" autocomplete="off"></textarea>
      </div>

      <button type="submit" class="btn btn-primary " >Publish</button>
</form>
<%- include('partials/footer.ejs') %>
